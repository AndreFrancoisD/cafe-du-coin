{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",   
    "declaration": false,  
    "sourceMap": false,
    "outDir": "./dist",   
    "strict": true,    
    "noImplicitThis": true,
    "skipLibCheck": true,
    "esModuleInterop": true,   
    "resolveJsonModule": true,      
    "forceConsistentCasingInFileNames": true   },
  "include": [
    "src/**/*"
  ]
}